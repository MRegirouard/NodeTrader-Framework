{
  "compilerOptions":
  {
    "outDir": "./built",
    "allowJs": true,
    "moduleResolution": "node",
    "module": "commonjs",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "target": "es2017",
    "esModuleInterop": true,
    "preserveConstEnums": true
  },
  "include": ["./src/**/*", "./pairs/**/*"],
}